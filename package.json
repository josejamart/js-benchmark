{
  "name": "js-benchmark",
  "version": "0.0.1",
  "description": "js-benchmark for Backbone and React",
  "scripts": {
    "watch": "set NODE_ENV=development && webpack-dev-server --open --config ./webpack/webpack.config.dev.js",
    "prebundle": "npm install",
    "bundle": "set NODE_ENV=production && webpack --progress --config ./webpack/webpack.config.prod.js",
    "predist":"npm run bundle",
    "dist": "http-server ./dist -p 3000 -a 0.0.0.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "backbone": "^1.3.3",
    "jquery": "2.1.4",
    "underscore": "^1.8.3",
    "moment": "^2.17.1",
    "handlebars": "^4.0.6"
  },
  "devDependencies": {
    "@types/backbone": "^1.3.3",
    "@types/handlebars": "^4.0.6",
    "@types/jquery": "^2.0.40",
    "@types/react": "^0.14.46",
    "@types/react-dom": "^0.14.18",
    "@types/underscore": "^1.7.36",
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "copy-webpack-plugin": "^4.0.1",
    "path": "^0.12.7",
    "css-loader": "^0.26.2",
    "style-loader": "^0.13.2",
    "typescript": "^2.1.5",
    "typings": "^2.0.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.3.0",
    "http-server":"^0.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josejamart/js-benchmark.git"
  },
  "keywords": [
    "Backbone",
    "React",
    "benchmark"
  ],
  "author": "Jose Javier and Quinto",
  "license": "MIT"
}
